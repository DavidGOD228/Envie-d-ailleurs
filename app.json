{
  "expo": {
    "name": "envie-d-ailleurs",
    "slug": "envie-d-ailleurs",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/AppIcon.png",
    "splash": {
      "image": "./assets/AppIcon.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "4",
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "envie-d-ailleurs",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ipapp.enviedailleurs"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
